{ "manifest_version": 2,
  "name": "Data Saver Proxy for Firefox",
  "version": "3.0",

 
  "icons": {
    "48": "icon/icon48.png"
  }

,
"background": {
  "scripts": ["counter.js"]
  
}
,

"browser_action": {
  "default_icon": {
    "19": "icon/icon16.png",
    "38": "icon/icon32.png"
  
  },
  "default_popup": "popup/tool.html",

  "default_title": "Google Data Saver Proxy"
},
"permissions": [
  "<all_urls>",
  "webRequest",
  "webRequestBlocking",
  "storage",
  "proxy",
  "storage"
]

}
